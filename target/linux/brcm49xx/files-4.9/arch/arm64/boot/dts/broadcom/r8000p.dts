#include "bcm4908.dtsi"

/ {
	model = "Netgear R8000P";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		bootargs = "earlyprintk console=ttyS0,115200n8";
		linux,stdout-path = "serial0:115200n8";
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x20000000>;
	};

	leds {
		compatible = "gpio-leds";
		#address-cells = <1>;
		#size-cells = <0>;

		/* GPIO base is 448 (ARCH_NR_GPIOS - ngpio) = (512 - 64) = 448 */

		power-white {
			label = "brcm49xx:white:power";
			gpios = <&gpio0 456 GPIO_ACTIVE_LOW>; /* GPIO 8 */
			default-state = "on";
		};

		power-amber {
			label = "brcm49xx:amber:power";
			gpios = <&gpio0 457 GPIO_ACTIVE_LOW>; /* GPIO 9 */
			default-state = "off";
		};

		wps {
			label = "brcm49xx:white:wps-btn";
			gpios = <&gpio0 458 GPIO_ACTIVE_LOW>; /* GPIO 10 */
			default-state = "off";
		};

		2ghz {
			label = "brcm49xx:white:2ghz";
			gpios = <&gpio0 462 GPIO_ACTIVE_LOW>; /* GPIO 14 */
			default-state = "off";
		};

		5ghz-1 {
			label = "brcm49xx:white:5ghz-1";
			gpios = <&gpio0 463 GPIO_ACTIVE_LOW>; /* GPIO 15 */
			default-state = "off";
		};

		5ghz-2 {
			label = "brcm49xx:white:5ghz-2";
			gpios = <&gpio0 464 GPIO_ACTIVE_LOW>; /* GPIO 16 */
			default-state = "off";
		};

		usb2 {
			label = "brcm49xx:white:usb2";
			gpios = <&gpio0 465 GPIO_ACTIVE_LOW>; /* GPIO 17 */
			default-state = "off";
		};

		usb3 {
			label = "brcm49xx:white:usb3";
			gpios = <&gpio0 466 GPIO_ACTIVE_LOW>; /* GPIO 18 */
			default-state = "off";
		};

		wireless {
			label = "brcm49xx:white:wifi-btn";
			gpios = <&gpio0 504 GPIO_ACTIVE_LOW>; /* GPIO 56 */
			default-state = "off";
		};
	};
};

&nand {
	nandcs@0 {
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <1024>;

		nvram@0 {
			reg = <0x0 0x100000>;
		};

		bootfs@100000 {
			reg = <0x100000 0x4a0000>;
			read-only;
		};

		firmware@5a0000 {
			reg = <0x5a0000 0x3f60000>;
		};

		/* misc1 ~ misc7 */
		netgear@4500000 {
			reg = <0x4500000 0x1200000>;
			read-only;
		};

		data@5700000 {
			reg = <0x5700000 0x2800000>;
		};
	};
};
